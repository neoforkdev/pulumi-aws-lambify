{
  "name": "@neofork/lambify",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "build:check": "npm run lint && npm run test && npm run build",
    "clean": "rm -rf dist",
    "dev": "tsx examples/basic/index.ts",
    "playground": "tsx playground.ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "vitest --run",
    "test:unit": "vitest tests/unit --run",
    "test:integration": "vitest tests/integration --run",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neoforkdev/pulumi-aws-lambify.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neoforkdev/pulumi-aws-lambify/issues"
  },
  "homepage": "https://github.com/neoforkdev/pulumi-aws-lambify#readme",
  "description": "",
  "dependencies": {
    "@apidevtools/swagger-parser": "^11.0.1",
    "@pulumi/aws": "^6.81.0",
    "@pulumi/pulumi": "^3.171.0",
    "yaml": "^2.8.0",
    "zod": "^3.25.57"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.15.21",
    "@types/swagger-parser": "^4.0.3",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.1.4"
  }
}
